{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/payments/(.*)",
            "dest": "/public/payments/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/storage/(.*)",
            "dest": "/api/index.php",
            "headers": {
                "X-Forwarded-Proto": "https"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "headers": {
                "X-Forwarded-Proto": "https"
            }
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "APP_LOG_LEVEL": "error",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "build": {
        "env": {
            "COMPOSER_FLAGS": "--no-dev --optimize-autoloader"
        }
    }
}
